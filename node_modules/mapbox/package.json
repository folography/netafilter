    "browser-stdout": "1.3.0",
    "browserify": "^11.0.0",
    "documentation": "^3.0.0",
    "envify": "^3.4.0",
    "eslint": "^0.24.1",
    "geojson-random": "^0.2.2",
    "smokestack": "^3.3.0",
    "tap": "^1.3.1",
    "tap-status": "^1.0.1",
    "tape": "^4.2.0",
    "uglifyjs": "^2.4.10"
  },
